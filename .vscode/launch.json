{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Debug: npm run start:debug (port 9230)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    },
    {
      "name": "Attach to 9230",
      "type": "node",
      "request": "attach",
      "port": 9230,
      "restart": false,
      "protocol": "inspector",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
